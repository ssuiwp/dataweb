<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swp.dataweb.dao.ItemMapper">

    <insert id="addItem">
        insert into
            user_item
        (user_id, item_id)
        values
        (#{userId}, #{itemId})
    </insert>

</mapper>
